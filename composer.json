{
    "name": "marshmallow/live-update",
    "description": "Edit your Nova resources from the index page.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "laravel/nova": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Marshmallow\\LiveUpdate\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Marshmallow\\LiveUpdate\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
